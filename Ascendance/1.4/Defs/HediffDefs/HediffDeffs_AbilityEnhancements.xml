<Defs>

  <!--Dodge-->

  <HediffDef ParentName="addedPartEnhanced">
    <defName>Ascendance_AbilityEnhanced_Dodge</defName>
    <label>Enhanced Foot Dodge</label>
    <description>A foot Enhancement to Dodge.</description>
    <descriptionHyperlinks>
      <ThingDef>Ascendance_AbilityEnhancement_Dodge</ThingDef>
    </descriptionHyperlinks>
    <comps>
      <li Class="HediffCompProperties_SeverityPerDay">
        <severityPerDay>0.49</severityPerDay>
      </li>
    </comps>
    <stages>
      <li>
        <label>Reforming</label>
        <minSeverity>0.25</minSeverity>
        <painOffset>0.5</painOffset>
        <restFallFactorOffset>0.25</restFallFactorOffset>
        <hungerRateFactorOffset>0.5</hungerRateFactorOffset>
        <partEfficiencyOffset>-0.1</partEfficiencyOffset>
        <capMods>
          <li>
            <capacity>Consciousness</capacity>
            <setMax>0.1</setMax>
          </li>
        </capMods>
      </li>
      <li>
        <label>Enhancing</label>
        <minSeverity>0.75</minSeverity>
        <painOffset>0.5</painOffset>
        <hungerRateFactorOffset>1</hungerRateFactorOffset>
        <capMods>
          <li>
            <capacity>Consciousness</capacity>
            <setMax>0.5</setMax>
          </li>
        </capMods>
        <statOffsets>
          <MeleeDodgeChance>1</MeleeDodgeChance>
        </statOffsets>
      </li>
      <li>
        <minSeverity>0.99</minSeverity>
        <statOffsets>
          <MeleeDodgeChance>5</MeleeDodgeChance>
        </statOffsets>
      </li>
    </stages>
  </HediffDef>

  <ThingDef ParentName="Ascendance_EnhancementBase">
    <defName>Ascendance_AbilityEnhancement_Dodge</defName>
